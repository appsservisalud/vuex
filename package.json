{
  "name": "assistans-counter-vuex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "vbuild": "vue-cli-service build",
    "start": "nodemon --watch ./src -e html,js,css,scss,vue --exec \"npm run build\" ",
    "build": "parcel build --no-scope-hoist ./src/index.html --dist-dir ./appsscript && npm run clean",
    "clean": "rimraf ./appsscript/*.css  ./appsscript/*.map",
    "sass": "sass --watch src/assets/scss/:src/assets/css/",
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gpull": "clasp pull",
    "gpush": "clasp deploy --deploymentId   AKfycbwm7ic83WUDWOA3D3IJTVJml0lzokWz_pHE7QY3t29QOAkIzzRGtgwAhwh0zX4nNnb6XA --versionNumber 0 ",
    "gcreate": "clasp create --title \"Assistans\" --rootDir ./appsscript",
    "gstart": "clasp push",
    "gopen": "clasp open --webapp",
    "ghelp": "clasp --help",
    "gdeploys": "clasp deployments"
  },
  "dependencies": {
    "@types/google-apps-script": "^1.0.55",
    "cli-plugin-router": "^0.0.1",
    "core-js": "^3.8.3",
    "crypto-browserify": "^3.12.0",
    "nodemon": "^2.0.20",
    "os-browserify": "^0.3.0",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.7.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "parcel": "^2.7.0"
  }
}
